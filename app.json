{
  "expo": {
    "name": "Ideas y diseño de uñas bonitas",
    "slug": "nails-designs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-unas.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-unas.png",
      "resizeMode": "contain",
      "backgroundColor": "#E5BBD3"
    },
    "scheme": "nails-designs",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.paulaymanu113.nailsdesigns",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-unas.png",
        "backgroundColor": "#E5BBD3"
      },
      "versionCode": 8,
      "package": "com.paulaymanu113.nailsdesigns",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "546c7b97-fc98-4f86-84af-4c2234c44547"
      }
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Permite a Diseño de uñas acceder a tus fotos para descargar la imagen.",
          "savePhotosPermission": "Permite a Diseño de uñas acceder a tus fotos para descargar la imagen.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3738413299329691~3139248746",
          "iosAppId": "ca-app-pub-3738413299329691~3139248746"
        }
      ],
      "expo-font"
    ]
  }
}
